type Product {
    id: ID!
    name: String!
    productType: Int!
    firm: String!
    productVersion: String!
}

type Puchase {
    id: ID!
    product: Product!
    seller: Seller!
    price: Float

}

type Seller {
    id: ID!
    name: String!
    searchUrl: String!

}

input ProductFilter {
    name: String
    productType: String
    firm: String
    productVersion: String
}