type Product {
    id: ID!
    name: String!
    productType: Int!
    firm: String!
    productVersion: String!
}

type Purchase {
    id: ID!
    product: Product!
    seller: Seller!
    price: Float
    productUrl: String

}

type Seller {
    id: ID!
    name: String!
    searchUrl: String!

}

input ProductFilter {
    name: String
    productType: String
    firm: String
    productVersion: String
}

input PurchaseFilter {
    product: String
    seller: String
    firm: String
}