type Product {
    id: ID!
    name: String!
    productType: Int!
    firm: String!
    productVersion: String!
}

type Puchase {
    id: ID
    product: Product
    seller: Seller

}

type Seller {
    id: ID
    shortName: String

}

input ProductFilter {
    name: String
    productType: String
    firm: String
    productVersion: String
}